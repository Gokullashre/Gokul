import java.util.*;
class Main {
    public static void main(String[] args) {
        int [][] a={{5,4,7},{1,8,3},{9,6,2}};
        int r=a.length;
        int c=a[0].length;
        for(int i=0;i<r;i++){
            for(int j=0;j<c;j++){
                a[j][i]=a[i][j];
            }
        }
          for(int i=0;i<r;i++){
            for(int j=0;j<c;j++){
               System.out.print(a[i][j]+" ");
            }
             System.out.println();
        }
        
    }
}
